{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended", "google"
  ],
  "rules": {
    "arrow-parens": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "no-trailing-spaces": "off",
    "padded-blocks": "off"
  }
}